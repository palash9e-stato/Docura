{
  "name": "RAG-chat-workflow",
  "nodes": [
    {
      "parameters": {
        "options": {}
      },
      "type": "@n8n/n8n-nodes-langchain.chatTrigger",
      "typeVersion": 1.3,
      "position": [0, 0],
      "id": "chat-trigger",
      "name": "When chat message received",
      "webhookId": "REPLACE_WEBHOOK_ID"
    },

    {
      "parameters": {
        "options": {}
      },
      "type": "@n8n/n8n-nodes-langchain.agent",
      "typeVersion": 3,
      "position": [208, 0],
      "id": "ai-agent",
      "name": "AI Agent"
    },

    {
      "parameters": {
        "model": {
          "mode": "list",
          "value": "gpt-4.1-mini"
        },
        "options": {}
      },
      "type": "@n8n/n8n-nodes-langchain.lmChatOpenAi",
      "typeVersion": 1.2,
      "position": [80, 208],
      "id": "openai-chat-model",
      "name": "OpenAI Chat Model",
      "credentials": {
        "openAiApi": "={{ $env.OPENAI_API_KEY }}"
      }
    },

    {
      "parameters": {},
      "type": "@n8n/n8n-nodes-langchain.memoryBufferWindow",
      "typeVersion": 1.3,
      "position": [224, 208],
      "id": "simple-memory",
      "name": "Simple Memory"
    },

    {
      "parameters": {
        "mode": "retrieve-as-tool",
        "toolDescription": "Retrieves relevant data about 'Attention is All You Need' and returns it in simple, structured language.",
        "pineconeIndex": {
          "value": "REPLACE_PINECONE_INDEX",
          "mode": "id"
        },
        "options": {}
      },
      "type": "@n8n/n8n-nodes-langchain.vectorStorePinecone",
      "typeVersion": 1.3,
      "position": [400, 240],
      "id": "pinecone-vector-store",
      "name": "Pinecone Vector Store",
      "credentials": {
        "pineconeApi": "={{ $env.PINECONE_API_KEY }}"
      }
    },

    {
      "parameters": {
        "options": {}
      },
      "type": "@n8n/n8n-nodes-langchain.embeddingsOpenAi",
      "typeVersion": 1.2,
      "position": [288, 368],
      "id": "embeddings-openai",
      "name": "Embeddings OpenAI",
      "credentials": {
        "openAiApi": "={{ $env.OPENAI_API_KEY }}"
      }
    }
  ],

  "pinData": {},

  "connections": {
    "When chat message received": {
      "main": [[{ "node": "AI Agent", "type": "main", "index": 0 }]]
    },
    "OpenAI Chat Model": {
      "ai_languageModel": [[{ "node": "AI Agent", "type": "ai_languageModel", "index": 0 }]]
    },
    "Simple Memory": {
      "ai_memory": [[{ "node": "AI Agent", "type": "ai_memory", "index": 0 }]]
    },
    "Pinecone Vector Store": {
      "ai_tool": [[{ "node": "AI Agent", "type": "ai_tool", "index": 0 }]]
    },
    "Embeddings OpenAI": {
      "ai_embedding": [[{ "node": "Pinecone Vector Store", "type": "ai_embedding", "index": 0 }]]
    }
  },

  "active": false,
  "settings": {
    "executionOrder": "v1"
  },
  "versionId": "REPLACE_VERSION_ID",
  "meta": {
    "instanceId": "REPLACE_INSTANCE_ID"
  },
  "id": "REPLACE_WORKFLOW_ID",
  "tags": []
}
